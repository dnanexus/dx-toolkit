{
    "filters": {
        "assay_filter": {
            "id": "testid",
            "name": "testname",
            "compound": [
                {
                    "filters": {
                        "allele$allele_type": {
                            "condition": "in",
                            "values": [
                                "SNP",
                                "Del",
                                "Ins"
                            ]
                        },
                        "allele$alt_freq": {
                            "condition": "between",
                            "values": [
                                0.1,
                                0.5
                            ]
                        },
                        "logic": "and",
                        "name": "allele"
                    }
                },
                {
                    "filters": {
                        "genotype$type": {
                            "condition": "in",
                            "values": [
                                "het",
                                "hom-alt"
                            ]
                        },
                        "logic": "and",
                        "name": "genotype"
                    }
                },
                {
                    "compound": [
                        {
                            "filter": {
                                "allele$chr": {
                                    "condition": "is",
                                    "values": 1
                                },
                                "allele$pos": {
                                    "condition": "less-than",
                                    "values": 20000
                                },
                                "logic": "and"
                            }
                        },
                        {
                            "filter": {
                                "allele$chr": {
                                    "condition": "is",
                                    "values": "X"
                                },
                                "allele$pos": {
                                    "condition": "less-than",
                                    "values": 1700
                                },
                                "logic": "and"
                            }
                        },
                        {
                            "name": "location"
                        },
                        {
                            "logic": "or"
                        }
                    ]
                }
            ]
        }
    },
    "logic": "and"
}