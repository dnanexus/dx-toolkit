{ "name": "benchmark",
  "title": "benchmark",
  "summary" : "benchmark for dx-upload and dx-download",
  "runSpec": {
    "file": "run.sh",
    "interpreter": "bash",
    "distribution": "Ubuntu",
    "release": "20.04",
    "version": "0"
  },
  "inputSpec": [
    {"name": "parallel", "class": "boolean", "optional": true},
    {"name": "ref",  "class": "array:file"}
  ],
  "outputSpec": [
    {"name": "results", "class": "array:file"}
  ],
  "access": {
    "network": ["*"]
  },
  "regionalOptions": {
    "aws:us-east-1": {
      "systemRequirements": {
        "*": {
          "instanceType": "mem2_ssd1_x2"
        }
      }
    },
    "aws:ap-southeast-2": {
      "systemRequirements": {
        "*": {
          "instanceType": "mem2_ssd1_x2"
        }
      }
    },
    "aws:eu-central-1": {
      "systemRequirements": {
        "*": {
          "instanceType": "mem2_ssd1_x2"
        }
      }
    },
    "azure:westus": {
      "systemRequirements": {
        "*": {
          "instanceType": "azure:mem2_ssd1_x2"
        }
      }
    }
  }
}
